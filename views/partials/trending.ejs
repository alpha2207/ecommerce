<% let trending=['usb-cable','r8','c25','narzo30']; %>
<section id="trending">
    <div class="row">
        <div class="col-12">
            <h3>Trending Now</h3>
        </div>

        <% trending.forEach(e=>{ %>
        <div class="col-lg-3 col-md-6">
            <div class="card" style="width: 18rem;">
                <img src=<%=`imgs/${e}.png`%> class="card-img-top" alt="...">
                <div class="card-body">
                  <h5 class="card-title"></h5>
                  <h5>Rs. 1000/-</h5>
                </div>
              </div>
        </div>
        <% }) %>
    </div>
</section>